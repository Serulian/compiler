from anotherfile import SomeStruct

class EmptyClass {}

class NonRequiredClass {
	var<int?> SomeField
	var<bool> AnotherField = false
}

class RequiredClass {
	var<int> SomeField
}

struct GenericStruct<T> {
	SomeThing T
}

struct WithDefaults {
	SomeField int = 2
	AnotherField string = 'hello world'
}

function<void> DoSomething() {
	/* empty */ (EmptyClass{})
	/* nonempty */(NonRequiredClass{})

	/* nonesome */(NonRequiredClass{
		SomeField: 2,
	})

	/* noneanother */(NonRequiredClass{
		AnotherField: true,
	})

	/* somestruct */(SomeStruct{})

	/* required */(RequiredClass{
		SomeField: 6,
	})

	var generic = /* generic */(GenericStruct<int>{
		SomeThing: 42,
	})

	/* genericmodified */(generic{
		SomeThing: 43,
	})

	/* withdefaults */(WithDefaults{})

	/* withdefaults2 */(WithDefaults{
		SomeField: 2,
	})
}