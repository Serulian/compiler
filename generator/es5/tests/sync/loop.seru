from "../../../../testlib/basictypes" import IntStream

function<int> DoSomething(i int) {
	return i + 1
}

function<any> TEST() {
	var counter = 0
	var stream = IntStream.OverRange(1, 2)
	for {
		var result = stream.Next()
		if !(result.Second!) {
			break
		}

		counter = counter + DoSomething(result.First ?? 0)
	}

	return counter == 5
}