import maimport
from maimport import AnotherFunction
from maimport import AnotherFunction as af

class SomeClass {
	var<int> someInt = 2
	var<bool> someBool = true

	constructor Build() {
		return SomeClass.new()
	}

	function<void> InstanceFunc() {
	}

	property<int> SomeProp {
		get { return this.someInt }
	}
}

function<void> DoSomething(sc SomeClass, scn SomeClass?) {
	sc.someInt
	SomeClass.Build

	sc->someInt
	SomeClass->Build

	scn?.someInt

	maimport.AnotherFunction
	AnotherFunction
	af

	sc.InstanceFunc()
	sc.InstanceFunc

	sc.SomeProp
	sc->SomeProp
	scn->SomeProp
}

function<any> TEST() {
	var sc = SomeClass.new()
	return sc.someBool && sc->someBool
}