class SomePromise {
	function<awaitable<bool>> Then(resolve function<void>(bool)) {
		resolve(true)
		return this
	}

	function<awaitable<bool>> Catch(rejection function<void>(error)) {
		return this
	}
}

function<bool?> DoSomething(p awaitable<bool>) {
   var<bool?> somebool
   somebool <- p
   return somebool
}

function<any> TEST() {
  return DoSomething(SomePromise.new())
}