class SomeClass {
	function<bool> DoSomething() {
		return true
	}
}

type MyType : SomeClass {
	function<bool> AnotherThing() {
		return SomeClass(this).DoSomething()
	}

	property<bool> SomeProp {
		get { return true }
	}
}

function<any> TEST() {
	var sc = SomeClass.new()
	var m = MyType(sc)
	return m.SomeProp && m.AnotherThing()
}