function<bool> DoSomethingAsync() {
	return true
}

class FirstClass {
	var<bool> SomeBool = <- DoSomethingAsync()
	function<void> DoSomething() {}
}

class SecondClass {
	var<bool> AnotherBool = false
	function<void> AnotherThing() {}
}

class MainClass : FirstClass + SecondClass {
	function<bool> DoSomething() {
		return this.SomeBool && !this.AnotherBool
	}
}

function<any> TEST() {
	return MainClass.new().DoSomething()
}