function<bool> SimpleFunction(props []{string}, children int*) {
	var counter = 0
	for value in children {
		counter = counter + value
	}
	return counter == 5
}

function<int> DoSomethingAsync(i int) {
	return i + 1
}

function<int> GetSomething(i int) {
	return <- DoSomethingAsync(i)
}

function<any> TEST() {
	return <SimpleFunction>
		{ GetSomething(1) }
		{ GetSomething(2) }
	</SimpleFunction>
}