struct SomeStruct {
    SomeProperty int
}

/**
 * Some really cool function
 */
function<int> SomeFunction(props SomeStruct) {
    return 2
}

/*
 * AnotherFunction without a 'doc' comment
 */
function<int> AnotherFunction(props []{string}) {
    return 3
}

// SomeDecorator loves normal comments!
function<int> SomeDecorator(value int) {
    return value
}

function<void> DoSomething() {
///  [sf    ] [sp        ]      [sd         ]
<SomeFunction SomeProperty={2} @SomeDecorator={true}/>

///  [af       ] [sp2       ]
<AnotherFunction SomeProperty="hello world"/>

<SomeFunction>
{
/// [isf]
'hello world'
}
</SomeFunction>
}